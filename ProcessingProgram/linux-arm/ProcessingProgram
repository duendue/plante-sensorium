#!/bin/sh

APPDIR=$(readlink -f "$0")
APPDIR=$(dirname "$APPDIR")
java -Djna.nosys=true -Djava.library.path="$APPDIR:$APPDIR/lib" -cp "$APPDIR:$APPDIR/lib/ProcessingProgram.jar:$APPDIR/lib/core.jar:$APPDIR/lib/gluegen-rt.jar:$APPDIR/lib/jogl-all.jar:$APPDIR/lib/SerialRecord.jar:$APPDIR/lib/controlP5.jar:$APPDIR/lib/oscP5.jar:$APPDIR/lib/jssc.jar:$APPDIR/lib/serial.jar" ProcessingProgram "$@"
